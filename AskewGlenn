function tunnel(new_q){
    e = campaignTrail_temp;
    return e.questions_json[e.questions_json.map(f=>f.pk).indexOf(new_q)];
}

cyoAdventure = function (a) {
    ans = campaignTrail_temp.player_answers[campaignTrail_temp.player_answers.length - 1]
    /// Askew ditches Adams for Williams
    if ((campaignTrail_temp.player_answers[1] == 3546)) {
        campaignTrail_temp.candidate_image_url = 'https://i.imgur.com/15wLG80.png';
        campaignTrail_temp.running_mate_image_url = 'https://i.imgur.com/EXJ1ThC.png';
    }
    /// adds Donna
    if ((campaignTrail_temp.player_answers[2] == 3549) || (campaignTrail_temp.player_answers[2] == 3550) || (campaignTrail_temp.player_answers[2] == 3551)) {
        campaignTrail_temp.candidate_image_url = 'https://i.imgur.com/2Uh86G0.png';
        campaignTrail_temp.running_mate_image_url = 'https://i.imgur.com/7Q8iKaO.png';
    }
    /// Buckley nominee
    if ((campaignTrail_temp.player_answers[3] == 3600)) {
        campaignTrail_temp.candidate_last_name = '';
        campaignTrail_temp.running_mate_last_name = '';
        campaignTrail_temp.candidate_image_url = 'https://files.catbox.moe/7cgbpt.png';
        const customStyling = document.createElement("style");
        customStyling.innerHTML = `
        #campaign_sign {
            background-image: url("https://i.imgur.com/q7mO6qy.jpg");
            background-size: cover;
            border-color: #c9c9c9;    
            border-width: .01em;
            margin: 0 auto;
            background-position: center;
            width: 100%; 
            height: 107px; 
            margin-left: -0.03em;
          }
          #map_container {
              background-image: url("");
              background-size: cover;
            }
            .inner_window_question h3 {
            background-color: #cfdfff;
              }
            .overlay_window h3 {
            background-color: #8ca3ff;
              }
          `;
      document.head.appendChild(customStyling);
    }
    /// remove Donna
    if((campaignTrail_temp.player_answers[5] == 3588) || (campaignTrail_temp.player_answers[5] == 3589)){
        campaignTrail_temp.running_mate_image_url = '';
    }
    /// Scoop Dem
    if((campaignTrail_temp.player_answers[5] == 3590)){
        campaignTrail_temp.candidate_json[1]["fields"].first_name = "Scoop";
        campaignTrail_temp.candidate_json[1]["fields"].last_name = "Jackson";
        campaignTrail_temp.question_number = 24;
    }
    /// VP Glenn
    if((campaignTrail_temp.player_answers[7] == 3604)){
        campaignTrail_temp.running_mate_image_url = 'https://files.catbox.moe/d70mdk.jpeg';
    }
    /// Askew VP part
    if ((campaignTrail_temp.player_answers[1] == 3545)) {
        askewvp = true;

        campaignTrail_temp.questions_json[campaignTrail_temp.question_number + 1] = tunnel(448);
        e.global_parameter_json[0].fields.question_count = 6

        campaignTrail_temp.candidate_json[0]["fields"].first_name = "Ronald";
        campaignTrail_temp.candidate_json[0]["fields"].last_name = "Reagan";
        campaignTrail_temp.candidate_json[1]["fields"].first_name = "Frank";
        campaignTrail_temp.candidate_json[1]["fields"].last_name = "Church";
        campaignTrail_temp.candidate_json[2]["fields"].first_name = "John";
        campaignTrail_temp.candidate_json[2]["fields"].last_name = "Hospers/Others";

        campaignTrail_temp.candidate_last_name = 'Church';
        campaignTrail_temp.candidate_image_url = 'https://i.imgur.com/zMyKTKA.png';
        campaignTrail_temp.running_mate_image_url = 'https://i.imgur.com/vQFarfU.png';
        campaignTrail_temp.election_json[0].fields.advisor_url = "https://i.imgur.com/N5IECe2.png";

        const style_overwrite = document.createElement("style");
            style_overwrite.innerHTML = 
            `#campaign_sign {
            background-color: #4a9da8;
            border-color: #002334;
            }`
            ;
        document.head.appendChild(style_overwrite);

        $("#game_window")[0].style.borderColor = "#002334" 
        $(".container")[0].style.backgroundColor = "#002334"
        $("#game_window")[0].style.backgroundColor = "#da9262"
        nct_stuff.themes[nct_stuff.selectedTheme].coloring_title = "#2E86C1"  
        nct_stuff.themes[nct_stuff.selectedTheme].coloring_window = "#da9262"
        document.body.background = "https://i.imgur.com/xVUELCI.jpg"
        document.getElementById("header").src = "https://i.imgur.com/CM0eghe.jpg";
    }
    if((campaignTrail_temp.player_answers[2] == 3664) || (campaignTrail_temp.player_answers[2] == 3665) || (campaignTrail_temp.player_answers[2] == 3666)){
        campaignTrail_temp.questions_json[campaignTrail_temp.question_number + 1] = tunnel(449);
    }
    if((campaignTrail_temp.player_answers[3] == 3667) || (campaignTrail_temp.player_answers[3] == 3668) || (campaignTrail_temp.player_answers[3] == 3669)){
        campaignTrail_temp.questions_json[campaignTrail_temp.question_number + 1] = tunnel(450);
    }
    if((campaignTrail_temp.player_answers[4] == 3670) || (campaignTrail_temp.player_answers[4] == 3671) || (campaignTrail_temp.player_answers[4] == 3672)){
        campaignTrail_temp.questions_json[campaignTrail_temp.question_number + 1] = tunnel(451);
